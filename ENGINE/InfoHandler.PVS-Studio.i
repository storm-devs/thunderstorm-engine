	{
		api->Trace("No service: dx9render");
		return false;
	}
	if( m_rs->IsInsideScene() ) {
		m_rs->MakePostProcess();
	}

	if( m_rs->GetRenderTarget(&m_pRenderTarget)!=((HRESULT)0L) || !m_pRenderTarget)
	{
		api->Trace("Can`t get render target");
		return false;
	}

	bool isOk = false;
	D3DSURFACE_DESC desc;
	if(m_pRenderTarget->GetDesc(&desc) == ((HRESULT)0L))
	{
		if(m_rs->CreateOffscreenPlainSurface(desc.Width, desc.Height, desc.Format, &m_pSurface) == ((HRESULT)0L))
		{
			if( DoPreOut() )
			{
				if(m_rs->GetRenderTargetData(m_pRenderTarget, m_pSurface) == ((HRESULT)0L))
				{
					isOk = true;
				}
			}
		}
	}
	if(!isOk)
	{
		api->Trace("Screen shot for info shower not created!");
		if(m_pSurface)
		{
			m_rs->Release(m_pSurface); m_pSurface=nullptr;
		}
		if(m_pRenderTarget)
		{
			m_rs->Release(m_pRenderTarget); m_pRenderTarget=nullptr;
		}
		return false;
	}

	return true;
}

